fn is_palindrome(x) {
    original = x
    reversed = 0
    while x > 0 {
        digit = x % 10
        reversed = reversed * 10 + digit
        x = x / 10
    }
    
    if original == reversed {
        return 1
    } else {
        return 0
    }
}

content = read_file("tests/resources/problems/palindrome.input")
lines = split(content, chr(10))
val1 = to_int(get(lines, 0))
val2 = to_int(get(lines, 1))

result1 = is_palindrome(val1)
result2 = is_palindrome(val2)
print(result1)
print(result2)
