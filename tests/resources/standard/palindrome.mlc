fn is_palindrome(x) {
    original = x
    reversed = 0
    while x > 0 {
        digit = x % 10
        reversed = reversed * 10 + digit
        x = x / 10
    }
    
    if original == reversed {
        return 1
    } else {
        return 0
    }
}

result1 = is_palindrome(121)
result2 = is_palindrome(123)
print(result1)
print(result2)
