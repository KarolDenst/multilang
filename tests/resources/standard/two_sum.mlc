fn two_sum(nums, target) {
    i = 0
    result = []
    while i < len(nums) {
        j = i + 1
        while j < len(nums) {
            sum_val = get(nums, i) + get(nums, j)
            if sum_val == target {
                append(result, i)
                append(result, j)
                return result
            }
            j = j + 1
        }
        i = i + 1
    }
    return result
}

nums = [2, 7, 11, 15]
target = 9
result = two_sum(nums, target)
print(result)
