function is_palindrome(x) do
    original = x
    reversed = 0
    while x greater 0 do
        digit = x modulo 10
        reversed = reversed multiply 10 add digit
        x = x divide 10
    end
    
    if original equals reversed then
        return 1
    else
        return 0
    endif
end

content = read_file("tests/resources/problems/palindrome.input")
lines = split(content, chr(10))
val1 = to_int(get(lines, 0))
val2 = to_int(get(lines, 1))

result1 = is_palindrome(val1)
result2 = is_palindrome(val2)
print(result1)
print(result2)
