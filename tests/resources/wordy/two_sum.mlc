function two_sum(nums, target) do
    i = 0
    result = []
    while i less len(nums) do
        j = i add 1
        while j less len(nums) do
            sum_val = get(nums, i) add get(nums, j)
            if sum_val equals target then
                append(result, i)
                append(result, j)
                return result
            endif
            j = j add 1
        end
        i = i add 1
    end
    return result
end

content = read_file("tests/resources/problems/two_sum.input")
lines = split(content, chr(10))
nums_str = get(lines, 0)
target_str = get(lines, 1)

nums_parts = split(nums_str, ",")
nums = []
k = 0
while k less len(nums_parts) do
    part = get(nums_parts, k)
    append(nums, to_int(part))
    k = k add 1
end

target = to_int(target_str)
result = two_sum(nums, target)
print(result)
