function fizzbuzz(n) do
    result = []
    i = 1
    while i less n add 1 do
        if i modulo 15 equals 0 then
            append(result, "FizzBuzz")
        else
            if i modulo 3 equals 0 then
                append(result, "Fizz")
            else
                if i modulo 5 equals 0 then
                    append(result, "Buzz")
                else
                    append(result, i)
                endif
            endif
        endif
        i = i add 1
    end
    return result
end

result = fizzbuzz(15)
print(result)
